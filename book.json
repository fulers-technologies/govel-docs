{
  "root": "./src",
  "title": "Govel Framework Documentation",
  "description": "Comprehensive documentation for the Govel Go framework - the Laravel-inspired framework for Go developers",
  "author": "Govel Team",
  "language": "en",
  "gitbook": ">=3.0.0",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "variables": {
    "version": "1.0.0",
    "framework_version": "1.0.0"
  },
  "plugins": [
    "search",
    "sharing",
    "fontsettings",
    "theme-default",
    "code",
    "prism",
    "anchors",
    "edit-link",
    "github",
    "sitemap"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/govel/docs/edit/main",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/govel/framework"
    },
    "sitemap": {
      "hostname": "https://fulers.gitbook.io/govel"
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "linkedin": true,
      "all": ["facebook", "twitter", "linkedin"]
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}

